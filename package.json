{
  "name": "cdo-sync",
  "displayName": "cdo-sync",
  "description": "Write code.org projects in typescript or es6 and sync to code.org",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cdo-sync.init",
    "onCommand:cdo-sync.setCookie",
    "onCommand:cdo-sync.setId",
    "onCommand:cdo-sync.start",
    "onCommand:cdo-sync.update",
    "onCommand:cdo-sync.syncMetadata",
    "onCommand:cdo-sync.syncSource",
    "onCommand:cdo-sync.syncAnimations"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cdo-sync.init",
        "title": "cdo-sync: Initialize project."
      },
      {
        "command": "cdo-sync.start",
        "title": "cdo-sync: Start project in watch mode."
      },
      {
        "command": "cdo-sync.stop",
        "title": "cdo-sync: Stop watch mode."
      },
      {
        "command": "cdo-sync.update",
        "title": "cdo-sync: Retrieve project."
      },
      {
        "command": "cdo-sync.syncSource",
        "title": "cdo-sync: Update source."
      },
      {
        "command": "cdo-sync.syncAnimations",
        "title": "cdo-sync: Update animations."
      },
      {
        "command": "cdo-sync.syncMetadata",
        "title": "cdo-sync: Update metadata."
      },
      {
        "command": "cdo-sync.setCookie",
        "title": "cdo-sync: Set project cookie."
      },
      {
        "command": "cdo-sync.setId",
        "title": "cdo-sync: Set project id."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.14",
    "@types/debounce": "^1.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "14.x",
    "@types/node-fetch": "^2.5.10",
    "@types/uuid": "^8.3.0",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "debounce": "^1.2.1",
    "form-data": "^3.0.1",
    "fs-extra": "^10.0.0",
    "image-size": "^1.0.0",
    "node-fetch": "^2.6.1",
    "uuid": "^8.3.2"
  }
}
