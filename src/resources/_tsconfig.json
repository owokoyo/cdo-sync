{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"jsx": "react",
		"jsxFactory": "Nomx.createElement",
		"lib": [
			"es5"
		],
		"typeRoots": [
			"./workspace"
		],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}